#!/bin/bash

# clearing old tmp files
rm -rf /tmp/dam-mmn/
# create directory to hold tmp files
mkdir /tmp/dam-mmn
mkdir /tmp/dam-mmn/1
mkdir /tmp/dam-mmn/1/MP3
mkdir /tmp/dam-mmn/1/OGG
mkdir /tmp/dam-mmn/2
mkdir /tmp/dam-mmn/2/MP3
mkdir /tmp/dam-mmn/2/OGG
# execute java program
java -cp out dam $@
exitVal=`echo $?`
# clear out all tmp files
rm -rf /tmp/dam-mmn/
exit $exitVal
